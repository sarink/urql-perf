{
  "compilerOptions": {
    "baseUrl": ".",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "jsx": "react-jsx",
    "lib": ["DOM", "DOM.Iterable", "ES2015"],
    "module": "ES2015",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "outDir": "dist",
    "strict": true,
    "target": "ES2015"
  },
  "exclude": ["../node_modules", "dist"]
}
